<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="FlowTagLayout">
        <attr name="ftl_item_spacing" format="dimension" />
    </declare-styleable>

    <declare-styleable name="HalfCircleView">
        <attr name="hcv_solid_color" format="color" />
        <attr name="hcv_stroke_width" format="dimension" />
        <attr name="hcv_stroke_color" format="color" />
        <attr name="hcv_direction" format="enum">
            <enum name="UP" value="1" />
            <enum name="DOWN" value="2" />
            <enum name="LEFT" value="3" />
            <enum name="RIGHT" value="4" />
        </attr>
    </declare-styleable>

    <declare-styleable name="YxCouponView">
        <attr name="yxc_stroke_width" format="dimension" />
        <attr name="yxc_stroke_color" format="color" />
        <attr name="yxc_corner_radius" format="dimension" />

        <attr name="yxc_left_text" format="string" />
        <attr name="yxc_left_text_horizontal_padding" format="dimension" />
        <attr name="yxc_right_text_horizontal_padding" format="dimension" />


        <attr name="yxc_left_bg_color" format="color" />
        <attr name="yxc_right_text" format="string" />
        <attr name="yxc_text_color" format="color" />
        <attr name="yxc_text_size" format="dimension" />
        <attr name="yxc_text_vertical_padding" format="dimension" />

    </declare-styleable>

</resources>